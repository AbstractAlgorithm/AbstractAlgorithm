<!doctype html>
<html lang="en">
	<head>
		<meta charset="UTF-8">

		<meta name="description" content="[block description]Abstract Algorithm - blog and portfolio of Dragan OkanoviÄ‡, web developer and VFX programmer.[end]" >
		<meta name="author" content="Okanovic Dragan" >
		<meta name="keywords" content="abstract, algorithm, blog, web, gpu, vfx, okanovic, dragan">
		<meta property="og:title" content="Abstract Algorithm | {title}"/>
		<meta property="og:image" content="http://abstract-algorithm.com/resources_aa/logo_big.png"/>
		<meta property="og:site_name" content="Abstract Algorithm | {title}"/>

		<title>{title} | Abstract Algorithm</title>

		<link rel="shortcut icon" href="{CONFIG_WEBSITE}/{CONFIG_IMG_DIR}/favicon.png" type="image/x-icon">
		<link rel="icon" href="{CONFIG_WEBSITE}/{CONFIG_IMG_DIR}/favicon.png" type="image/x-icon">
		<link rel="stylesheet" href="{CONFIG_WEBSITE}/{CONFIG_CSS_DIR}/[block css]style.css[end]">
	</head>
	
	<body>
		[block header]
		<header id="header">
			<!-- <a class="logo" href="{CONFIG_WEBSITE}">Abstract<br>Algorithm</a> -->
		    <nav>
		        <ul>
		        	<li>
		                <a href="{CONFIG_WEBSITE}/" [if headerMenu.0]class="orange"[end]><span class="stripe"></span>Home</a>
		            </li>
		            <li>
		                <a href="{CONFIG_WEBSITE}/blog" [if headerMenu.1]class="orange"[end]><span class="stripe"></span>Blog</a>
		            </li>
		            <li>
		                <a href="{CONFIG_WEBSITE}/category/tag/project" [if headerMenu.2]class="orange"[end]><span class="stripe"></span>Projects</a>
		            </li>
		            <li>
		                <a href="{CONFIG_WEBSITE}/category/tag/random" [if headerMenu.3]class="orange"[end]><span class="stripe"></span>Random</a>
		            </li>
		            <li>
		                <a href="{CONFIG_WEBSITE}/about" [if headerMenu.4]class="orange"[end]><span class="stripe"></span>About</a>
		            </li>
		            [if postPage]
		            <br><br>
		            <li>
		                <a href="#" id="vote"><span class="stripe"></span>Vote</a>
		            </li>
		            <li>
		                <a href="#"><span class="stripe"></span>Comments</a>
		            </li>
		            <li>
		                <a href="#"><span class="stripe"></span>Share</a>
		            </li>
		            [end]
		        </ul>
		    </nav>
		</header>
		[end]

		<div class="wrapper">

			<div id="content">
				[block content][end]

				[block footer][end]
			</div>
		</div>

		[block script]
		<script type="text/javascript" src="http://code.jquery.com/jquery-2.1.1.min.js"></script>
		<script type="text/javascript" src="{CONFIG_JS_DIR}/myCode.js"></script>
		[end]
	</body>
</html>